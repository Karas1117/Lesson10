*
  margin 0
  padding 0
  border 0
  list-style none
  text-decoration none
  box-sizing border-box
  font-family: main-font;

.wrapper
  padding 0 15px
  max-width 1200px
  margin 0 auto

.clear
  clear both

.text
  font-size 18px
  line-height: 32px;
  color grey-text

.bold
  font-weight 700

.titles
  font-size 43px
  font-weight 300
  color grey-text
  padding-bottom 20px

.green-text
  color green-text
